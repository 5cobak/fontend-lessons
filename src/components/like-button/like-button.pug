mixin like-button({count})
  button(type="button").like-button.js-like-button
    svg.like-button__like.js-like-button__like
      use(xlink:href="#like-not-active")
    svg.like-button__like-favorite.js-like-button__like-favorite
      use(xlink:href="#favorite")
    span.like-button__likes.js-like-button__likes #{count}