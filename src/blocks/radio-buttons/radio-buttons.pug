mixin radio-buttons({title}) 
  .radio-buttons
    span.radio-buttons__title #{title}
    label
      input(type="radio" name="radio")
      span.radio-buttons__checkmark
      span.radio-buttons__text Мужчина
    label
      input(type="radio" name="radio")
      span.radio-buttons__checkmark
      span.radio-buttons__text Женщина
